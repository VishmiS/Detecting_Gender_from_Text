   batch_embeddings = outputs.last_hidden_state[:, 0, :].numpy()\n","            embeddings.append(batch_embeddings)\n","    return np.vstack(embeddings)\n","\n","# Process data in batches\n","embeddings = get_bert_embeddings(texts, model, tokenizer)\n","\n","# Split data into training and validation sets\n","X_train, X_val, y_train, y_val = train_test_split(embeddings, labels, test_size=0.2, random_state=42)\n","\n","# Train an SVM classifier\n","svm = SVC()\n","svm.fit(X_train, y_train)\n","\n","# Make predictions on the validation set\n","y_pred = svm.predict(X_val)\n","\n","# Measure accuracy\n","accuracy = accuracy_score(y_val, y_pred)\n","print(f'Accuracy: {accuracy}')"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":371,"referenced_widgets":["7dd959adaa0b41fb9e74a6a7f1809d22","a8e2cd979c504c7692df030fe832cfa0","3b4f50c14c204b73b490d1b3f2815f8f","ac16dc35e1484a0185bb5dc8d4ff8754","17c6548d1652466a9b48ffcce05e1319","9d5694d6af3d42e6b16fe62a523458bb","39bf60e85d4f458c95e627612e13fbbb","3dd6926e7b0e42128558f596ad273b58","1794d568b42845129381f945f128217b","e2d49a90181848269f64354d557f42f8","1056664989dc454e91e4680b3cb5f8b1","7dfe2efad1ff4872b2ec4ab27b05903a","2b21d0ffeb8b41fca69496d3d2e59cc4","6913f544a1aa42778dcb4415749002ff","53a4db51178c44d68ce36300f4ab2fb4","effca6dcaab14b49876a94429a4959fb","351f9967eac645da84a932aedb2a29ea","bb7d14be53a549acb7a6038c9728fd16","46bdb3b91a864663865c8cc76160152c","4f6ef7e46a754274aa3d5402f8da12a7","daf53103c769424da7db39c3d034839e","9ad801e288184416a7a843af75735760","92d2a53c772c4719b7059aa42fd6c2e4","fa183f70f61d46c6beb43c1b8271176d","dbcd0d8f347b49afa7d347f78dbfd91f","6f1eccd38b824d468b2673d9684dc031","0ceee2a4ff9441d5a860bb88299697b9","0cc8096189f34bdab036e0e878fe7f5a","75d3291a9bc144089594ca9e4790405e","ee6260c671bb42a8b5fcf043c05759fd","cbd9510e67cf4e2da8b8f19c9918fe9a","2d3db5d9ca0947d68e4a9470ffa5f41d","cc5e0c3a9d604ae8a3fe78b0a0457311","1081ce9842964504bf8f00cf126ef596","b74604cd5411482985bafabd5c9462